{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AccessWardrobeImages",
      "Effect": "Allow",
      "Action": ["s3:DeleteObject", "s3:GetObject"],
      "Resource": "arn:aws:s3:::<<bucket-name>>/*"
    },
    {
      "Sid": "DeleteWardrobeItem",
      "Effect": "Allow",
      "Action": ["dynamodb:GetItem", "dynamodb:DeleteItem"],
      "Resource": "arn:aws:dynamodb:<<region>>:<<account-id>>:table/<<wardrobe-table-name>>"
    }
  ]
}
